  <%= turbo_frame_tag 'login' do %>
    
    <h2 class="text-white text-xl font-semibold mb-4">Log in</h2> 

    <%= form_for(resource, as: resource_name, url: session_path(resource_name), class: "w-full max-w-xs") do |f| %>
      <div class="field mb-3">
        <%= f.label :email, class: "block text-sm font-medium text-white" %>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "mt-1 block w-full bg-transparent text-white border border-gray-500 rounded-md p-1.5 focus-visible:outline-none" %>
      </div>
    
      <div class="field mb-3">
        <%= f.label :password, class: "block text-sm font-medium text-white" %>
        <%= f.password_field :password, autocomplete: "current-password", class: "mt-1 block w-full bg-transparent text-white border border-gray-500 rounded-md p-1.5 focus:outline-none" %>
      </div>
    
      <% if devise_mapping.rememberable? %>
        <div class="field mb-3 flex items-center">
          <%= f.check_box :remember_me, class: "h-3 w-3 text-indigo-600 border-gray-300 rounded" %>
          <%= f.label :remember_me, class: "ml-2 block text-sm text-white" %>
        </div>
      <% end %>
    
      <div class="actions flex flex-col gap-1">
        <%= f.submit "Log in", class: "w-full bg-[#1F1F24] text-white font-bold py-2 px-3 rounded-md hover:bg-indigo-700 hover:bg-gray-400 hover:text-black"%> 
      
        <%= render "devise/shared/links" %>          
      </div>         
    <% end %>
  <% end %>
